<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="main_window" named="True">
    <property name="background">#515460</property>
    <property name="geometry">800x480</property>
    <property name="height">200</property>
    <property name="resizable">none</property>
    <property name="title" translatable="yes">Youtube Downloader</property>
    <property name="width">200</property>
    <child>
      <object class="tk.Frame" id="frm_main" named="True">
        <property name="background">#515460</property>
        <property name="height">480</property>
        <property name="width">800</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Label" id="lbl_appname" named="True">
            <property name="anchor">n</property>
            <property name="background">#515460</property>
            <property name="compound">top</property>
            <property name="font">{Arial} 24 {bold}</property>
            <property name="foreground">#f9fafb</property>
            <property name="justify">left</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Youtube Downloader
</property>
            <layout manager="pack">
              <property name="pady">30</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="url_entry" named="True">
            <property name="font">{Arial} 12 {}</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">Βάλε link
</property>
            <property name="textvariable">string:url</property>
            <property name="width">60</property>
            <layout manager="pack">
              <property name="ipadx">30</property>
              <property name="pady">30</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="btn_download" named="True">
            <property name="activebackground">#ff0404</property>
            <property name="command" type="command" cbtype="simple">download_button</property>
            <property name="compound">center</property>
            <property name="font">{arial} 8 {}</property>
            <property name="text" translatable="yes">Download
</property>
            <property name="width">10</property>
            <layout manager="pack">
              <property name="pady">110</property>
              <property name="side">bottom</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
